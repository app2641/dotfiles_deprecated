  d4 = diff HEAD~~~~
  d5 = diff HEAD~~~~~
  empty= commit --allow-empty
  ft = fetch --prune
  fta = fetch --all --prune
  lg = log --graph --branches --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(green)- %an, %cr%Creset'
  ls = ls-files
  nm = merge --no-ff
  pl = pull --rebase
  psh = push
  pshf = push --force-with-lease
  st = status
  rb = rebase
  rba = rebase --abort
  rbc = rebase --continue
  rbi = rebase -i
  wip = !git add . && git commit -m 'wip' && git status

